{
    "name": "kingdomino-proto",
    "version": "1.0.0",
    "author": "Virgil King <virgilking@gmail.com>",
    "type": "module",
    "main": "out/kingdomino-proto.js",
    "scripts": {
        "protoc": "mkdir -p out; protoc --plugin=node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=out --ts_proto_opt=esModuleInterop=true --ts_proto_opt=importSuffix=.js --proto_path=. *.proto",
        "build": "pnpm run protoc && pnpm exec tsc",
        "clean": "rm -rf out/*"
    },
    "devDependencies": {
        "protobufjs": "^7.2.6",
        "ts-proto": "^1.172.0",
        "tsconfig": "workspace:*",
        "typescript": "^5.4.5"
    }
}
